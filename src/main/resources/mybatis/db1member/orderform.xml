<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mycompany.webapp.dao.db1member.OrderFormDao">

	<select id="selectByMid" parameterType="String" resultType="memberVo">
		select * from member m, grade g
		 where mid=#{mid} and m.grade_level = g.glevel
	</select>

	
</mapper>